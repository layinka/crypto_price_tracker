{
  "name": "Crypto Price Tracker",
  "version": "1.0",
  "description": "Display key coin spot prices",
  "manifest_version": 2,
  "permissions": [
      "activeTab",
      "webNavigation",
      "storage"
  ],
  "background": {
      "scripts": [
          "background.js",
          "runtime.js"
      ]
  },
  
  "browser_action": {
    "default_icon": "assets/omw_icon.png",
    "default_popup": "index.html"
   }
}
